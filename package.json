    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "mongodb": "^3.2.3",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
